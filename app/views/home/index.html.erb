<% if user_signed_in? %>
  <div class="row">
    <div class="col-4">

    </div>
    <div class="col-8">
      <h1><%= current_user.name %></h1>
      <div>
        <ul class="nav">
          <li><p><strong><%= @images.count %></strong> publications</p></li>
        </ul>
      </div>
    </div>
  </div>
  <div class="row" id="images">
    <%= render @images %>
  </div>
  <%= will_paginate @images %>
<% else %>
  <h1>Community</h1>
<% end %>